{
    "name": "dynamic/staff-directory",
    "description": "a directory of staff/employees with detail pages",
    "authors": [
        {
            "name": "Dynamic, Inc",
            "email": "dev@dynamicagency.com",
            "homepage": "http://www.dynamicagency.com"
        }
    ],
    "keywords": [
        "silverstripe", "dynamic", "staff", "directory"
    ],
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "require": {
        "littlegiant/silverstripe-catalogmanager": "dev-ss5",
        "silverstripe/lumberjack": "^3.0",
        "silverstripe/recipe-cms": "^5.0",
        "silverstripe/vendor-plugin": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:toastnz/silverstripe-catalogmanager.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dynamic\\Staff\\": "src/",
            "Dynamic\\Staff\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
